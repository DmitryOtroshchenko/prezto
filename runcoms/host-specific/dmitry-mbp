#
# Directory shortcuts
#

alias google='cd ~/Documents/workspace/google/'
alias ipdir='cd ~/Documents/workspace/ipython/'
alias thesis='cd ~/Documents/workspace/Thesis/src/'
alias madev='cd ~/Documents/workspace/ma/MeilleursAgents/'

#
# Aliases
